<?xml version="1.0" ?>
<style-scheme id="LeonLion" name="Leon Lion Style" version="1.0">
	<author/>
	<_description>LeonLion</_description>
	
	<color name="gray" value="#bbbbbb"/>
	<color name="white" value="#eeeeee"/>
	<color name="yellow" value="#fce94f"/>
	<color name="senape" value="#acc900"/>
	<color name="lime" value="#96ff00"/>
	<color name="green" value="#00c900"/>
	<color name="alga" value="#00c99b"/>
	<color name="aqua" value="#00d8ff"/>
	<color name="cyan" value="#009cff"/>
	<color name="violet" value="#9e91ff"/>
	<color name="purple" value="#bb66ff"/>
	<color name="fuchsia" value="#ff44cc"/>
	<color name="magenta" value="#ff79d9"/>
	<color name="red" value="#ff2f6a"/>
	<color name="orange" value="#ff6100"/>
	<color name="ambra" value="#ff9900"/>
	<color name="asfalto" value="#555753"/>
	<color name="carbon" value="#121212"/>

<!--
        GUI styles 
-->
	<style name="selection" foreground="asfalto" background="#181818"/>
	<style name="selection-unfocused" foreground="white" background="gray"/>
	<style name="cursor" foreground="white"/>
	<style name="secondary-cursor" foreground="magenta"/>
	<style name="current-line" background="#222222"/>
	<style name="line-numbers" foreground="asfalto" background="gray"/>
	<style name="bracket-match" foreground="white" background="#111111" bold="true"/>
	<style name="bracket-mismatch" foreground="white" background="magenta" bold="true"/>
	<style name="search-match" foreground="carbon" background="yellow"/>
	
	
	
	<style background="#333333" foreground="#cccccc" name="text"/>
	<style background="#601830" foreground="#e03870" name="diff:changed-line"/>
	<style background="#0020a0" foreground="#a0d0ff" name="diff:removed-line"/>
	<style background="#0020a0" foreground="#a0d0ff" name="diff:added-line"/>
	<style background="#222222" foreground="#cccccc" name="cursor"/>
	<style foreground="#707070" name="line-numbers"/>
	<style foreground="#666666" name="def:comment"/>
	<style foreground="#cccc66" name="def:constant"/>
	<style background="#8000ff" foreground="#ffffff" name="def:error"/>
	<style foreground="#6699cc" name="def:identifier"/>
	<style foreground="#cc99cc" name="def:preprocessor"/>
	<style foreground="#ffc890" name="def:specials"/>
	<style foreground="#cc9966" name="def:statement"/>
	<style foreground="#669966" name="def:type"/>
</style-scheme>

